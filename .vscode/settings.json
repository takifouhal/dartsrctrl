{
  "dart.flutterSdkPath": ".fvm/versions/3.7.9",
  "dart.sdkPath": ".fvm/versions/3.7.9/bin/cache/dart-sdk",
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "dart.analysisExcludedFolders": [
    ".fvm"
  ],
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.rulers": [
    80
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  }
}